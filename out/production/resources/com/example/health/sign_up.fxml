<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.health.sign_upColtroller">
   <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
   </rowConstraints>
   <children>
      <AnchorPane prefHeight="446.0" prefWidth="520.0">
         <children>
            <AnchorPane prefHeight="82.0" prefWidth="520.0" style="-fx-background-color: #043730;">
               <children>
                  <Label layoutX="139.0" layoutY="27.0" prefHeight="29.0" prefWidth="242.0" text="sign up for your health" textFill="#ff6a00">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Label>
               </children>
            </AnchorPane>
            <Label layoutX="92.0" layoutY="89.0" text="Name">
               <font>
                  <Font name="JetBrains Mono SemiBold" size="13.0" />
               </font>
            </Label>
            <TextField fx:id="name_textfield" layoutX="92.0" layoutY="107.0" prefHeight="25.0" prefWidth="336.0" promptText="Name" />
            <Label layoutX="92.0" layoutY="139.0" text="Phone Number">
               <font>
                  <Font name="JetBrains Mono SemiBold" size="13.0" />
               </font>
            </Label>
            <TextField fx:id="phone_textfield" layoutX="92.0" layoutY="157.0" prefHeight="25.0" prefWidth="336.0" promptText="Phone Number" />
            <Label layoutX="92.0" layoutY="193.0" text="User Name(ID)">
               <font>
                  <Font name="JetBrains Mono SemiBold" size="13.0" />
               </font>
            </Label>
            <TextField fx:id="id_textfield" layoutX="92.0" layoutY="211.0" prefHeight="25.0" prefWidth="336.0" promptText="User Name(ID)" />
            <Label layoutX="94.0" layoutY="245.0" text="Password">
               <font>
                  <Font name="JetBrains Mono SemiBold" size="13.0" />
               </font>
            </Label>
            <PasswordField fx:id="pass_textfield" layoutX="92.0" layoutY="263.0" prefHeight="25.0" prefWidth="336.0" promptText="Password" />
            <Label layoutX="94.0" layoutY="299.0" text="Confirm Password">
               <font>
                  <Font name="JetBrains Mono SemiBold" size="13.0" />
               </font>
            </Label>
            <PasswordField fx:id="repass_textfield" layoutX="92.0" layoutY="317.0" prefHeight="25.0" prefWidth="336.0" promptText="Confirm Password" />
            <Button fx:id="signup_button" layoutX="96.0" layoutY="365.0" onAction="#signupButtonAction" mnemonicParsing="false" prefHeight="22.0" prefWidth="336.0" style="-fx-background-color: #043730;" text="Sign UP!" textFill="#e18848">
               <font>
                  <Font name="JetBrains Mono SemiBold" size="13.0" />
               </font>
            </Button>
            <Button fx:id="close_button" layoutX="96.0" layoutY="404.0" onAction="#closeButtonAction" mnemonicParsing="false" prefHeight="22.0" prefWidth="336.0" style="-fx-background-color: #043730;" text="Close" textFill="#e18848">
               <font>
                  <Font name="JetBrains Mono SemiBold" size="13.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
   </children>
</GridPane>
