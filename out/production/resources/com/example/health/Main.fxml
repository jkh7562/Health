            <BorderPane layoutX="-1.0" layoutY="-1.0" prefHeight="400.0" prefWidth="600.0">
               <left>
                  <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER" />
               </left>
               <top>
                  <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
                     <children>
                        <Pane prefHeight="200.0" prefWidth="200.0">
                           <children>
                              <Pane layoutX="201.0" layoutY="-100.0" prefHeight="198.0" prefWidth="286.0" rotate="35.0" style="-fx-background-color: #ed6637; -fx-background-radius: 70;" />
                           </children>
                        </Pane>
                     </children>
                  </VBox>
               </top>
            </BorderPane>
            <Pane prefHeight="400.0" prefWidth="593.0">
               <children>
                  <VBox layoutX="115.0" layoutY="-7.0" prefHeight="407.0" prefWidth="484.0" style="-fx-background-color: #f8f2e6;">
