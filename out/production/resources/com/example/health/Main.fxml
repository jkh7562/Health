                              <VBox prefHeight="200.0" prefWidth="100.0" />
                           </children>
                        </HBox>
                        <HBox prefHeight="95.0" prefWidth="471.0">
                           <children>
                              <VBox prefHeight="200.0" prefWidth="100.0" />
                              <VBox prefHeight="200.0" prefWidth="100.0" />
                              <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
                                 <children>
                                    <Button mnemonicParsing="false" prefHeight="79.0" prefWidth="80.0" style="-fx-background-radius: 20; -fx-background-color: #043730;" text="add friend" textFill="WHITE" />
                                 </children></VBox>
                              <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
                                 <children>
                                    <Button mnemonicParsing="false" prefHeight="79.0" prefWidth="80.0" style="-fx-background-radius: 20; -fx-background-color: #043730;" text="?" textFill="WHITE" />
                                 </children></VBox>
                              <VBox prefHeight="200.0" prefWidth="100.0" />
                           </children>
