                              <Image url="@../../../brand%20logo.png" />
                           </image></ImageView>
                        <HBox prefHeight="73.0" prefWidth="133.0" />
                        <HBox prefHeight="81.0" prefWidth="133.0">
                           <children>
                              <Button fx:id="page_button" alignment="CENTER_RIGHT" mnemonicParsing="false" onAction="#pageButtonAction" prefHeight="36.0" prefWidth="133.0" style="-fx-background-radius: 30; -fx-background-color: #ed6637;" styleClass="backgroundColor" text="My Page">
                                 <font>
                                    <Font name="JetBrains Mono SemiBold" size="18.0" />
                                 </font></Button>
                           </children>
                        </HBox>
                        <HBox prefHeight="100.0" prefWidth="200.0">
                           <children>
                              <Button fx:id="logout_button" alignment="CENTER_RIGHT" mnemonicParsing="false" onAction="#logoutButtonAction" prefHeight="36.0" prefWidth="133.0" style="-fx-background-radius: 30; -fx-background-color: #ed6637;" styleClass="backgroundColor" text="Log Out">
                                 <font>
                                    <Font name="JetBrains Mono SemiBold" size="18.0" />
                                 </font>
                              </Button>
                           </children>
                        </HBox>
                     </children>
                  </VBox>
               </children>
            </Pane>
         </children>
      </AnchorPane>
   </children>
</GridPane>
