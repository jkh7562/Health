                        <HBox alignment="CENTER" prefHeight="85.0" prefWidth="470.0">
                           <children>
                              <Label text="Select your menu">
                                 <font>
                                    <Font name="JetBrains Mono SemiBold" size="24.0" />
                                 </font>
                              </Label>
                           </children></HBox>
                        <HBox prefHeight="140.0" prefWidth="471.0">
                           <children>
                              <VBox prefHeight="200.0" prefWidth="100.0" />
