                                 <children>
                                    <Button fx:id="exe_button" mnemonicParsing="false" onAction="#exeButtonAction" prefHeight="79.0" prefWidth="80.0" style="-fx-background-radius: 20; -fx-background-color: #043730;" text="exercise" textFill="WHITE" />
                                 </children>
                              </VBox>
                              <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
                                 <children>
                                    <Button fx:id="cal_button" mnemonicParsing="false" onAction="#calButtonAction" prefHeight="79.0" prefWidth="80.0" style="-fx-background-radius: 20; -fx-background-color: #043730;" text="Calorie" textFill="WHITE" />
                                 </children></VBox>
                              <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
                                 <children>
                                    <Button fx:id="daily_button" onAction="#dailyButtonAction" mnemonicParsing="false" prefHeight="79.0" prefWidth="80.0" style="-fx-background-radius: 20; -fx-background-color: #043730;" text="daily" textFill="WHITE" />
