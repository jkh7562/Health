                        <HBox prefHeight="84.0" prefWidth="484.0" />
                     </children>
                  </VBox>
                  <VBox alignment="TOP_RIGHT" layoutX="-26.0" prefHeight="400.0" prefWidth="164.0" style="-fx-background-color: #043730;">
                     <children>
                        <ImageView fitHeight="75.0" fitWidth="160.0" pickOnBounds="true" preserveRatio="true">
